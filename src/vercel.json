{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/primeng",
        "installCommand": "npm install --legacy-peer-deps --force --no-audit",
        "buildCommand": "NODE_OPTIONS=--openssl-legacy-provider npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_OPTIONS": "--openssl-legacy-provider"
  }
}